class Solution {
    public int missingNumber(int[] nums) {
        int ans=0;
        int n = nums.length;
        int arr[]=new int[n+1];
        for(int i=0;i<=n;i++){
            arr[i]=i;   
        }
        Arrays.sort(nums);
        for(int i=0;i<n;i++){
            if(nums[i]==arr[i]){

            }
        
        else{
            ans=arr[i];
        }
        }
        return ans;
        
    }
}
